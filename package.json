{
  "name": "edit-media",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "cross-env VITE_BUILD_TARGET=web vite",
    "dev:website": "cross-env VITE_DEV=true VITE_IS_WEBSITE=true vite",
    "sourcemaps": "npx @sentry/wizard -i sourcemaps",
    "build": "tsc && cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build",
    "build:web": "tsc && cross-env VITE_BUILD_TARGET=web vite build",
    "build-website": "tsc && cross-env VITE_IS_WEBSITE=true vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:app": "tauri build",
    "build-86": "tauri build -t x86_64-apple-darwin",
    "build-linux": "tauri build -t x86_64-unknown-linux-gnu",
    "build-win": "tauri build -t x86_64-pc-windows-msvc",
    "build-win32": "tauri build -t i686-pc-windows-msvc",
    "build-arm": "tauri build -t aarch64-apple-darwin",
    "tauri-dev": "cross-env VITE_DEV=true TAURI_DEV_WATCHER_IGNORE_FILE=.tauriignore tauri dev"
  },
